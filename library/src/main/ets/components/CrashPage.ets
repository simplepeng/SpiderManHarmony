import { Const } from './Const';

@Entry({ routeName: Const.CRASH_PAGE })
@Component
export struct CrashPage {
  @State message: string = 'Hello World';

  build() {
    Row() {
      Column() {
        Text(this.message)
          .fontSize(50)
          .fontWeight(FontWeight.Bold)
      }
      .width('100%')
    }
    .height('100%')
  }
}
